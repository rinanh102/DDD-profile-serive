{
    "name": "ddd",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "dev": "nodemon -L --config nodemon.debug.json",
        "build": "node build.js && tsc --build tsconfig.json",
        "debug": "npm run bdev && nodemon --config nodemon.debug.json",
        "bdev": "node build.js && tsc --build tsconfig.debug.json",
        "start": "node dist/index.js",
        "test": "jest --updateSnapshot --detectOpenHandles",
        "migrate:make": "knex migrate:make",
        "migrate:up": "knex migrate:up",
        "migrate:latest": "knex migrate:latest",
        "migrate:down": "knex migrate:down",
        "migrate:rollback": "knex migrate:rollback",
        "migrate:list": "knex migrate:list",
        "seed:make": "knex seed:make",
        "seed:run": "knex seed:run",
        "lint": "eslint --ext .js,.ts .",
        "lint:fix": "eslint --fix --ext .js,.ts .",
        "lint:format": "prettier --write .",
        "package": "npm run lint && npm run build && node module.js"
    },
    "keywords": [],
    "nodemonConfig": {
        "legacyWatch": true,
        "watch": [
            "src"
        ],
        "ext": "ts,html",
        "ignore": [
            "src/public"
        ],
        "exec": "ts-node src"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/multer": "^1.4.7",
        "@types/node": "^15.14.9",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.3.1",
        "@typescript-eslint/parser": "^5.3.1",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "fs-extra": "^10.0.0",
        "inversify": "^5.1.1",
        "jest": "^27.1.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.4.1",
        "sqlite3": "^5.0.2",
        "ts-node": "^10.8.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@cbidigital/aqua": "2.0.9",
        "@cbidigital/nightcore": "^1.3.5",
        "@cbidigital/profile-module": "^1.2.7",
        "@heronjs/common": "^2.3.9",
        "@heronjs/core": "^2.2.4",
        "@heronjs/express": "^2.1.1",
        "@heronjs/websocket": "^2.0.3",
        "axios": "^0.27.2",
        "cache-manager": "^3.4.4",
        "cache-manager-ioredis": "^2.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-handlebars": "^5.3.2",
        "http-status-codes": "^2.2.0",
        "ioredis": "^4.27.8",
        "knex": "^2.1.0",
        "lodash": "^4.17.21",
        "module-alias": "^2.2.2",
        "pg": "^8.7.3",
        "progress": "^2.0.3",
        "qs": "^6.11.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.1.0",
        "socket.io": "^4.5.1",
        "tslib": "^2.2.0",
        "uuid": "^8.3.2"
    }
}
